# MockMate Frontend Production Environment Configuration
# Frontend: https://mock-mate.com

# Build Configuration
NODE_ENV=production
GENERATE_SOURCEMAP=false
CI=true

# API Configuration
VITE_API_BASE_URL=https://backend.mock-mate.com/api
VITE_BACKEND_URL=https://backend.mock-mate.com
VITE_FRONTEND_URL=https://mock-mate.com

# Firebase Configuration (UPDATE WITH PRODUCTION VALUES)
VITE_FIREBASE_API_KEY=your-production-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-production-firebase-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-production-firebase-project
VITE_FIREBASE_STORAGE_BUCKET=your-production-firebase-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-firebase-sender-id
VITE_FIREBASE_APP_ID=your-firebase-app-id
VITE_FIREBASE_MEASUREMENT_ID=your-firebase-measurement-id

# Stripe Configuration (UPDATE WITH PRODUCTION VALUES)
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_production_stripe_publishable_key

# Google Analytics / Tracking (if applicable)
VITE_GA_MEASUREMENT_ID=your-google-analytics-id
VITE_GTAG_ID=your-gtag-id

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_DEBUG_MODE=false

# App Configuration
VITE_APP_NAME=MockMate
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI-Powered Mock Interview Platform

# CDN/Asset Configuration (if using CDN)
VITE_CDN_URL=https://cdn.mock-mate.com
VITE_ASSETS_URL=https://assets.mock-mate.com

# Social Media / SEO
VITE_SOCIAL_TWITTER=@mockmate
VITE_SOCIAL_LINKEDIN=mockmate
VITE_CONTACT_EMAIL=support@mock-mate.com

# Security
VITE_SECURE_COOKIES=true
VITE_CSRF_PROTECTION=true

# Performance
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true

# Monitoring (if using services like Sentry)
VITE_SENTRY_DSN=your-sentry-dsn-if-applicable
VITE_ENABLE_ERROR_BOUNDARY=true
